{
  "cli": {
    "appVersionSource": "local"
  },
  "build": {
    "production": {
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleRelease"
      },
      "env": {
        "npm_config_legacy_peer_deps": "true",
        "JAVA_HOME": "/usr/lib/jvm/java-17-openjdk-amd64"
      }
    },
    "preview": {
      "android": {
        "buildType": "apk"
      },
      "env": {
        "npm_config_legacy_peer_deps": "true",
        "JAVA_HOME": "/usr/lib/jvm/java-17-openjdk-amd64"
      }
    }
  }
}
